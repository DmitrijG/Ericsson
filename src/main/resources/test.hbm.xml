<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ericsson.ams.model.Test" table="t_test">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>
		<version name="version" type="java.lang.Long" access="field"
			column="object_version" />
		<property name="a" type="java.lang.Integer" column="a" />
		<property name="b" type="java.lang.String" column="b" />
	</class>
</hibernate-mapping>